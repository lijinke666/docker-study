{
  "name": "docker-study",
  "version": "2.2.3",
  "description": "react project template By:dawdler",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "yarn dev",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production && yarn run clean && webpack --env.mode=production --mode production --progress --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development && yarn run clean && webpack-dev-server --mode development --progress --config webpack.config.js",
    "upgrade": "yarn upgrade",
    "precommit": "lint-staged",
    "lint": "prettier --write \"src/**/*.js\" && eslint_d --fix src",
    "test": "jest __tests__/tests",
    "coverage": "yarn test -- --coverage",
    "ci": "yarn coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "pre-commit": "lint",
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint_d --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lijinke666/react-project-template.git"
  },
  "keywords": [
    "dawdler",
    "webpack",
